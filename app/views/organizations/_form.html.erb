
<%= form_for (@organization) do |f| %>
	<div class="field">
		<%= f.label :name %><br>
		<%= f.text_field :name %>
	</div>

	<div class="field">
		<%= f.label :description %><br>
		<%= f.text_field :description %>
	</div>


	<div class="field">
		<%= f.label :phone %><br>
		<%= f.text_field :phone %>
	</div>

	<div class="field">
		<%= f.label :email %><br>
		<%= f.text_field :email %>
	</div>

	<div class="field web">
		<%= f.label :website %><br>
		http://	<%= f.text_field :website %>
	</div>

	<div class="field-btn">
		<%= f.submit  "Create Organization" %>
	</div>
<% end %>
