<h1>New Organization </h1>

<%= form_for (@organization) do |f| %>
	<div>
		<%= f.label :name %>
		<%= f.text_field :name %>
	</div>

	<div>
		<%= f.label :description %>
		<%= f.text_field :description %>
	</div>

	<div>
		<%= f.label :contact %>
		<%= f.text_field :contact %>
	</div>

	<div>
		<%= f.label :phone %>
		<%= f.text_field :phone %>
	</div>

	<div>
		<%= f.label :email %>
		<%= f.text_field :email %>
	</div>

	<div>
		<%= f.label :website %>
		http://	<%= f.text_field :website %>
	</div>

	<div>
		<%= f.submit  "Create Organization" %>
	</div>
<% end %>
